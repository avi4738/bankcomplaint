	<script>
    $(document).ready(function(){
        //===== | STICKY HEADER | =====\\
        $header = $('header');
        var $window = $(window);
        if ($window.scrollTop() > 200) {
            $header.addClass('sticky');
        }
        $window.scroll(function () {
            if ($(this).scrollTop() > 200) {
                if (!$('header.sticky').length) {
                    $header.addClass('sticky');
                }
            }
            else {
                $header.removeClass('sticky');
            }
        });

        var $alert = $('.main-alert');
        if ($alert.length) {
            var $mainBody = $('.main-body');
            var alertHeight = $alert.outerHeight();
            $header.css('margin-top', alertHeight);
            $mainBody.css('padding-top', alertHeight + 80);
            $('.modal-dialog').css('margin-top', alertHeight + 130);
            $window.resize(function () {
                var alertHeightChange = $alert.outerHeight();
                $header.css('margin-top', alertHeightChange);
                $mainBody.css('padding-top', alertHeightChange + 80);
            });
        }
    });
</script>


